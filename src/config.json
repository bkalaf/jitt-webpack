    "buttons": {
        "login": "Login",
        "logout": "Logout",
        "register": "Register",
        "forgot": "Forgot your password?"
    },
    "modals": {
        "register": {
            "form": {
                "onsubmit": "",
                "fieldset": {
                    "email": {
                        "type": "email",
                        "tag": "input"
                    },
                    "password": {
                        "type": "password",
                        "tag": "input",
                        "minlength": 8,
                        "data-validation-contains": [
                            [
                                ["GTE", 1], "lower-alpha"
                            ],
                            [
                                ["GTE", 1], "upper-alpha"
                            ],
                            [
                                ["GTE", 1], "digit"
                            ]
                        ]
                    },
                    "verifyPassword": {
                        "type": "password",
                        "tag": "input",
                        "data-validation-equalto": "password"
                    }
                },
                "buttons": [
                    "cancel",
                    "reset",
                    "submit"
                ]
            }
        },
        "login": {
            "form": {
                "onsubmit": "app.auth().signInWithEmailAndPassword",
                "fieldset": {
                    "email": {
                        "type": "email",
                        "tag": "input",
                        "placeholder": "Enter your e-mail address"
                    },
                    "password": {
                        "type": "password",
                        "tag": "input",
                        "placeholder": "Enter your password",
                        "minlength": 8

                    }
                },
                "buttons": [
                    "cancel",
                    "reset",
                    "submit"
                ]
            }

        },
        "verifyEmail": {

        },
        "forgotPassword": {

        }
    }
}